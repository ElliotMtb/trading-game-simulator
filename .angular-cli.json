{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "settlers-app"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "assets": [
        "assets",
        "favicon.ico"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "tsconfig.spec.json",
      "prefix": "app",
      "styles": [
        "styles.css"
      ],
      "scripts": [
          "scripts/third-party/jquery-1.10.2.min.js",
          "scripts/third-party/underscore-min.js",
          "scripts/third-party/backbone-min.js",
          "scripts/third-party/backbone.localStorage-min.js",
          "scripts/third-party/kinetic-v4.5.5.min.js",
          "scripts/settlers_spa.js",
          "scripts/linked-list_basic.js",
          "scripts/linked-list_circular.js",
          "scripts/game_play_machine.js",
          "scripts/utility.js",
          "scripts/board_data.js",
          "scripts/models/player_model.js",
          "scripts/models/catanhex_model.js",
          "scripts/models/intersection_model.js",
          "scripts/views/intersection_view.js",
          "scripts/views/catanhex_view.js",
          "scripts/views/player_view.js",
          "scripts/views/setup_view.js",
          "scripts/router.js",
          "scripts/pieces-builder.js",
          "scripts/game_board_controller.js",
          "scripts/controlpanel_controller.js",
          "scripts/hex_builder.js",
          "scripts/intersection_builder.js",
          "scripts/game_board_builder.js",
          "scripts/mainview_initializer.js"
        ],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "src/tsconfig.app.json"
    },
    {
      "project": "src/tsconfig.spec.json"
    },
    {
      "project": "e2e/tsconfig.e2e.json"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "css",
    "component": {}
  }
}
